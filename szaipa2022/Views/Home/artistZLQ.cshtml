<script>
    // 导航栏的焦点切换
    $(window).scroll(function () {
        var scrollDistance = $(window).scrollTop();
        $('section').each(function (i) {
            if ($(this).position().top <= scrollDistance) {
                $('#ArtistNav .activeLink').removeClass('activeLink');
                $('#ArtistNav td').eq(i + 1).addClass('activeLink');
            }
        });
    }).scroll();

    // 背景图的悬浮更换
    $(function () {
        $('.hoverLine').hover(function () {
            var index = $('.hoverLine').index(this); // 获取当前 li 元素的下标
            var bgImg = '';

            switch (index) {
                case 0:
                    bgImg = '../../content/ArtImg/Artist/Works/zhanglanqian-01.jpg';
                    break;
                case 1:
                    bgImg = '../../content/ArtImg/Artist/Works/zhanglanqian-02.jpg';
                    break;
                case 2:
                    bgImg = '../../content/ArtImg/Artist/Works/zhanglanqian-01.jpg';
                    break;
                case 3:
                    bgImg = '../../content/ArtImg/Artist/Works/zhanglanqian-02.jpg';
                    break;

            }

            $('#section01').css('background-image', 'url("' + bgImg + '")');
        }, function () {
            $('#section01').css('background-image', 'url("../../content/ArtImg/Artist/Works/zhanglanqian-01.jpg")');
        });
    });

    // 年表 - 折叠手风琴
    $(function () {
        // when page loads, the collapsible is collapsed
        $('.collapsible').each(function () {
            // get the content of the collapsible
            var content = $(this).next('.content');
            // if the collapsible is not collapsed by default
            if ($(this).attr('data-collapsed') === 'false') {
                // expand the collapsible
                content.css('max-height', content.prop('scrollHeight') + 'px');
                $(this).addClass('defaultActive');
            }
        });

        // when the collapsible is clicked
        $('.collapsible').click(function () {
            // get the content of the collapsible
            var content = $(this).next('.content');
            // if the collapsible is collapsed
            var collapsed = $(this).attr('data-collapsed') === 'true';

            // toggle the classes of the collapsible
            $(this).toggleClass('defaultActive');
            content.toggleClass('active').toggleClass('inactive');

            // expand or collapse the collapsible
            content.css('max-height', collapsed ? content.prop('scrollHeight') + 'px' : 0);
            $(this).attr('data-collapsed', collapsed ? 'false' : 'true');
        });

        // when the toggle button is clicked
        $('#toggleBtn').click(function () {
            // toggle all the collapsibles
            $('.collapsible').each(function () {
                // get the content of the collapsible
                var content = $(this).next('.content');
                // if the collapsible is collapsed
                var collapsed = $(this).attr('data-collapsed') === 'true';

                // expand or collapse the collapsible
                content.css('max-height', collapsed ? content.prop('scrollHeight') + 'px' : 0);
                // toggle the classes of the collapsible
                $(this).toggleClass('defaultActive', collapsed);
                // change the collapsed attribute
                $(this).attr('data-collapsed', collapsed ? 'false' : 'true');
            });
        });
    });

    // Swiper
    $(function () {
        var deviceWidth = $(window).width(); // 获取设备宽度
        var deviceHeight = $(window).height(); // 获取设备高度

        var deviceRatio = deviceWidth / deviceHeight; // 计算设备宽高比
        var slidesPerView = deviceRatio > 1 ? 2 : 1; // 判断宽高比并设置预览数量

        var swiper = new Swiper(".mySwiper", {
            spaceBetween: 0,
            slidesPerView: slidesPerView,
            navigation: {
                nextEl: ".swiper-button-next",
                prevEl: ".swiper-button-prev",
            },
            keyboard: {
                enabled: true,
                onlyInViewport: true,
            },
        });

        var swiper2 = new Swiper(".mySwiper2", {
            direction: "vertical",
            spaceBetween: 50,
            pagination: {
                el: ".swiper-pagination",
                clickable: true,
            },
        });

        // Swiper3
        var swiper3 = new Swiper(".mySwiper3", {
            spaceBetween: 0,
            pagination: {
                el: ".swiper-pagination",
                clickable: true,
            },
            autoplay: {
                delay: 2500,
                disableOnInteraction: false,
            },
            loop: true,
        });

    });
</script>

