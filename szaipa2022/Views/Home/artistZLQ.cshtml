            $('.hoverLine').hover(function () {
                var index = $('.hoverLine').index(this); // 获取当前 li 元素的下标
                var bgImg = '';

                switch (index) {
                    case 0:
                        bgImg = '/content/ArtImg/Artist/Works/zhanglanqian-01.jpg';
                        break;
                    case 1:
                        bgImg = '/content/ArtImg/Artist/Works/zhanglanqian-02.jpg';
                        break;
                    case 2:
                        bgImg = '/content/ArtImg/Artist/Works/zhanglanqian-01.jpg';
                        break;
