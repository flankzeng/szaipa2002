    <script>
        $(document).ready(function () {
            $(".floating-box").hide();
            $(window).on("load", function () {
                $(".floating-box").show();
            });

            $(".close-btn").on("click", function () {
                $(".floating-box").hide();
            });
        });

    </script>
    <style>
        .floating-box {
            position: fixed;
            bottom: 3vh;
            right: 1vw;
            background-color: #ededed;
            padding: 1em 2em;
            width: 30vw;
            z-index: 9999;
        }

        .close-btn {
            position: absolute;
            top: 50%;
            transform: translateY(-60%);
            right: 1vw;
            font-size: 2rem;
            font-weight: bold;
            cursor: pointer;
            color: #ccc;
        }

        .floating-box p {
            font-size: 1rem;
        }

        .close-btn:hover {
            color: #000;
        }
    </style>
