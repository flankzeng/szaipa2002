            margin-right: var(--navRight);
            margin-top: var(--navTop);
            text-decoration: none;
            color: black;
        }

        .topList {
            height: inherit;
            display: flex;
            float: right;
            list-style: none;
            margin: 0px;
            padding: 0px;
        }

        .lineL {
            height: inherit;
            border-left: 1px solid #877150;
        }

        .lineR {
            margin-right: 60px;
            height: inherit;
            border-right: 1px solid #877150;
        }


