            lazy: true,
            navigation: {
                nextEl: ".swiper-button-next",
                prevEl: ".swiper-button-prev",
            },
            thumbs: {
                swiper: swiper2,
            },
            keyboard: {
                enabled: true,
                onlyInViewport: true,
            },
        });

