<script>// section04的双swiper,02是子，03是父

    var swiper2 = new Swiper(".mySwiper2", {
        loop: true,
        grabCursor: true,
        spaceBetween: 3,
        slidesPerView: 7,
        freeMode: true,
        watchSlidesProgress: true,
    });
    var swiper3 = new Swiper(".mySwiper3", {
        loop: true,
        spaceBetween: 0,
        grabCursor: true,
        // slidesPerView: 0,
        // 启用懒加载
        lazy: true,
        navigation: {
            nextEl: ".swiper-button-next",
            prevEl: ".swiper-button-prev",
        },
        thumbs: {
            swiper: swiper2,
        },
        keyboard: {
            enabled: true,
            onlyInViewport: true,
        },
    });</script>

